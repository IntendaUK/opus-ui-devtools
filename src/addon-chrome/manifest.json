{
	"manifest_version": 2,
	"name": "Opus DevTools",
	"version": "1.1.0",
	"permissions": [
		"activeTab",
		"<all_urls>"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_start"
		}
	],
	"devtools_page": "devtools/devtools.html",
	"icons": {
		"48": "favicon.png"
	}
}